<section class="hero is-medium">
  <div class="hero-body has-background-primary">
    <h1 class="title is-1">BookMark</h1>
    <p class="subtitle is-3">Create your next reading list</p>
  </div>
</section>
<%= render partial: "shared/searchbar"%>
<% if @booksDisplay %>
  <div class="columns is-multiline is-centered">
    <%@booksDisplay.each do |oneBook| %>
      <%= render partial: "shared/bookCard",locals: {oneBook: oneBook}%> <% end %>
  </div>
<%end%>
